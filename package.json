{
  "name": "bluenet-nodejs-lib",
  "version": "0.0.1",
  "scripts": {
    "start": "tsc -watch",
    "test": "jest --useStderr --clearMocks",
    "cleanTest": "jest --useStderr --no-cache --clearMocks"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "dependencies": {
    "aes-js": "^3.1.1",
    "jest": "^22.4.4",
    "noble": "^1.9.1"
  },
  "devDependencies": {
    "@types/node": "^10.0.8",
    "@types/jest": "^22.2.3",
    "typescript": "^2.8.3"
  },
  "jest": {
    "moduleFileExtensions": ["ts", "tsx", "js"],
    "transform": {
      "^.+\\.(ts|tsx)$": "./tests/jest_preprocessor.js"
    },
    "testMatch": ["**/tests/*.test.+(ts|tsx|js)"],
    "setupFiles": [
      "./tests/jest_setup.js"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": []
  },
  "license": "MIT"
}
